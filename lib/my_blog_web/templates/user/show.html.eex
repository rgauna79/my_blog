<h1>Show User</h1>

<ul>

  <li>
    <strong>Fist Name:</strong>
    <%= @user.f_name %>
  </li>

  <li>
    <strong>Last name:</strong>
    <%= @user.l_name %>
  </li>

  <li>
    <strong>Password:</strong>
    <%= @user.password_hash %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
